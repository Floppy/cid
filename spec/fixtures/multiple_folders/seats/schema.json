{
  "title": "UK European Election Data - Seats",
  "description": "Seats won by party in a UK European Election",
  "fields": [
      {
          "name": "Poll Date",
          "type": "date",
          "description": "Date the poll took place",
          "constraints": {
            "format": "yyyy-mm-dd",
            "required": true
          }
      },
      {
          "name": "Area",
          "type": "string",
          "description": "The name of the area which the result is for",
          "constraints": {
            "required": true
          }
      },
      {
          "name": "Area Code",
          "type": "number",
          "description": "The Office of National Statistics code for the area",
          "constraints": {
            "required": true,
            "pattern": "(E|N|S|W)(07|15|08)00000[0-9]"
          }
      },
      {
          "name": "Area URL",
          "type": "string",
          "description": "",
          "constraints": {
            "required": true,
            "pattern": "http://statistics.data.gov.uk/doc/statistical-geography/(E|N|S|W)(07|15|08)00000[0-9]"
          }
      },
      {
          "name": "Seat",
          "type": "integer",
          "description": "",
          "constraints": {
            "required": true
          }
      },
      {
          "name": "Party",
          "type": "string",
          "description": "",
          "constraints": {
            "required": true
          }
      },
      {
          "name": "Party ID",
          "type": "string",
          "description": "",
          "constraints": {
            "required": true,
            "pattern": "PPm? [0-9]+"
          }
      },
      {
          "name": "Name",
          "type": "string",
          "description": "Name of the candidate elected to the seat",
          "constraints": {
            "required": true
          }
      },
      {
          "name": "Address",
          "type": "string",
          "description": "Address of the candidate elected to the seat",
          "constraints": {
            "required": true
          }
      },
      {
          "name": "Postcode",
          "type": "string",
          "description": "Postcode of the candidate elected to the seat",
          "constraints": {
            "pattern": "(GIR 0AA)|((([A-Z-[QVX]][0-9][0-9]?)|(([A-Z-[QVX]][A-Z-[IJZ]][0-9][0-9]?)|(([A-Z-[QVX]][0-9][A-HJKSTUW])|([A-Z-[QVX]][A-Z-[IJZ]][0-9][ABEHMNPRVWXY])))) [0-9][A-Z-[CIKMOV]]{2})"
          }
      }
  ]
}
